<h1>Travelers</h1>

<% @users.each do |user_instance|%>
    <% if user_instance != current_user %>
<li><%= link_to user_instance.username, user_path(user_instance.id) %>
<% end%>
<% end %>


<% @users.each do |user_instance|%>
    <% if user_instance != current_user %>
<div class="row row-cols-1 row-cols-md-3">
    <div class="card">
      <img src="..." class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title" ><%= link_to user_instance.name, user_path(user_instance.id) %></h5>
      </div>
    </div>
  </div>
<% end%>
<% end%>




