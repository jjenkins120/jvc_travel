<h1>Posts#edit</h1>



<%if flash[:errors]%>
    <%flash[:errors].each do |error|%>
    <p style="color: red"><%= error%></p>
    <%end%>
<%end%>


<%= form_for @post do |f| %>


<p>Post Details</p>
	<%= f.label :title %>
	<%= f.text_field :title%><br><br>
    <%= f.label :description %><br>
    <%= f.text_area :description%><br><br>
    
	<%= f.submit %>

<% end %>